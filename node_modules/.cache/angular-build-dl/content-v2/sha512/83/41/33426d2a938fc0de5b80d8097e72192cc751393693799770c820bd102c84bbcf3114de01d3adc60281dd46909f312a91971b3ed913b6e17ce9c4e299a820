eteFoodById(id: number) {\n    return firebase.firestore().collection(this.collectionName).where('id' , '==' , id).onSnapshot(x => {\n        \n      let allData = [];\n      x.docs.forEach(\n        y =>{\n          y.ref.delete();\n        }\n      )\n    });\n  }\n\n  updateFoodItem(foodItem){\n      return firebase.firestore().collection(this.collectionName).where('id' , '==' ,foodItem.id).onSnapshot(x => {\n        \n        let allData = [];\n        x.docs.forEach(\n          y =>{\n            console.log(y)\n            y.ref.update(foodItem);\n          }\n        )\n        this.router.navigate(['/edit-food']);\n      });\n  }\n}\n","import { Injectable } from '@angular/core';\n\n\n\ninterface user{\n  username: string,\n  uid: string\n}\n@Injectable({\n  providedIn: 'root'\n})\nexport class UserService {\n  private user: user\n  constructor() { }\n\n  setUser(user: user){\n    console.log(user)\n    this.user = user\n  }\n\n  getUID(){\n    return this.user;\n  }\n}\n"],"sourceRoot":"webpack:///"}