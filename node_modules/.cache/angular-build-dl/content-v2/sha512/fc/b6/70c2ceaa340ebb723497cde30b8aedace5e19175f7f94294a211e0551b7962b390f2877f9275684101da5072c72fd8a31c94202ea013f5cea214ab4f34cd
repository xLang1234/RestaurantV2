/* harmony import */ var _services_user_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../services/user.service */ "./src/app/services/user.service.ts");





let LoginPage = class LoginPage {
    constructor(toastCtrl, loadingCtl, afAuth, navCtl, UserService) {
        this.toastCtrl = toastCtrl;
        this.loadingCtl = loadingCtl;
        this.afAuth = afAuth;
        this.navCtl = navCtl;
        this.UserService = UserService;
        this.user = {};
    }
    ngOnInit() {
    }
    login(user) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            if (this.formValidation()) {
                let loader = this.loadingCtl.create({
                    message: "Please wait ..."
                });
                (yield loader).present();
                try {
                    const res = yield this.afAuth.signInWithEmailAndPassword(user.email, user.password);
                    if (res.user) {
                        this.UserService.setUser({ username: user.email,
                            uid: res.user.uid });
                        if (res.user.email == 'test@email.com') {
                            this.navCtl.navigateRoot("admin");
                        }
                        else {
                            this.navCtl.navigateRoot("home");
                        }
                    }
                }
                catch (error) {
                    this.showToast(error);
                }
                (yield loader).dismiss();
            }
        });
    }
    formValidation() {
        if (!this.user.email) {
            this.showToast("Enter Email");
            return false;
        }
        if (!this.user.email) {
            this.showToast("Enter Password");
            return false;
        }
        return true;
    }
    showToast(message) {
        this.toastCtrl.create({
            message: message,
            duration: 3000
        }).then(toastData => toastData.present());
    }
};
